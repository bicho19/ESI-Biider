                   <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
              <children>
                  <JFXCheckBox mnemonicParsing="false" text="ID" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <JFXCheckBox mnemonicParsing="false" text="First Name" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <JFXCheckBox mnemonicParsing="false" text="Last Name" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <JFXCheckBox mnemonicParsing="false" text="Birth Date" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <JFXCheckBox mnemonicParsing="false" text="Birth Place" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <JFXCheckBox mnemonicParsing="false" text="Address" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="4" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="5" />
                  <JFXTextField GridPane.columnIndex="2" GridPane.rowIndex="6" />
                 <Label text="Search By :" />
              </children>
           </GridPane>
           <JFXButton layoutX="85.0" layoutY="301.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="Search"
                      buttonType="RAISED" style="-fx-text-fill:WHITE;-fx-background-color:#4CAF50;-fx-font-size:14px;"/>
       </AnchorPane>
   </center>
