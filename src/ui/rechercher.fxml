<BorderPane onMouseDragged="#setOnMouseDragged" onMousePressed="#setOnMousePressed" prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.rechercher_controller">
    <top>
        <ToolBar fx:id="toolBar" onMouseDragged="#setOnMouseDragged" onMousePressed="#setOnMousePressed"
                 prefHeight="57.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <items>
                <Pane prefHeight="0.0" prefWidth="785.0">
                    <fx:define>
                        <Image fx:id="btnImage" url="/ui/images/close.png" />
                    </fx:define>
                    <JFXButton fx:id="btnMinim" LayoutX="729" alignment="TOP_RIGHT"
                               mnemonicParsing="false" onAction="#minimizeButton" buttonType="RAISED">
                        <graphic>
                            <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                                <image>
                                    <Image url="/ui/images/minim.png" />
                                </image>
                            </ImageView>
                        </graphic>
                    </JFXButton>
                    <JFXButton fx:id="btnClose" LayoutX="754" alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#closeButtonAction"
                               buttonType="RAISED">
                        <graphic>
                            <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                                <image>
                                    <Image url="/ui/images/close.png" />
                                </image>
                            </ImageView>
                        </graphic>
                    </JFXButton>
               <ImageView fitHeight="47.0" fitWidth="200.0" pickOnBounds="true" smooth="false">
                  <image>
                     <Image url="@../../../Desktop/Untitled-2.png" />
                  </image>
               </ImageView>
                </Pane>
            </items>
        </ToolBar>
    </top>
    <left>
        <VBox id="vbox" alignment="TOP_CENTER" prefHeight="376.0" prefWidth="180.0" BorderPane.alignment="CENTER">
            <children>
                <JFXButton id="mydb" fx:id="mydb" mnemonicParsing="false"
                           prefHeight="30.0" prefWidth="180.0" text="My DB" buttonType="FLAT" alignment="CENTER_LEFT"
                           onMouseMoved="#onMydbMoved" onMouseExited="#onMydbLeave">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                            <image>
                                <Image url="/ui/images/close.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton id="settings" fx:id="settings" mnemonicParsing="false"
                           prefHeight="30.0" prefWidth="180.0" text="Settings" buttonType="FLAT" alignment="CENTER_LEFT"
                           onMouseMoved="#onSettingMoved" onMouseExited="#onSettingLeave">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                            <image>
                                <Image url="/ui/images/settings.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton id="help" fx:id="help" mnemonicParsing="false"
                           prefHeight="30.0" prefWidth="180.0" text="Help" buttonType="FLAT" alignment="CENTER_LEFT"
                           onMouseMoved="#onHelpMoved" onMouseExited="#onHelpLeave">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                            <image>
                                <Image url="/ui/images/help.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton id="about" fx:id="about" mnemonicParsing="false"
                           prefHeight="30.0" prefWidth="180.0" text="About" buttonType="FLAT" alignment="CENTER_LEFT"
                           onMouseMoved="#onAboutMoved" onMouseExited="#onAboutLeave">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" nodeOrientation="RIGHT_TO_LEFT">
                            <image>
                                <Image url="/ui/images/about.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
            </children>
        </VBox>
    </left>
   <center>
       <AnchorPane id="anchorpane" prefHeight="303.0" prefWidth="470.0" BorderPane.alignment="CENTER">
           <GridPane alignment="TOP_RIGHT" layoutY="19.0" prefHeight="255.0" prefWidth="417.0">
               <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="93.0" minWidth="40.0" prefWidth="67.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="186.0" minWidth="40.0" prefWidth="136.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="214.0" minWidth="40.0" prefWidth="214.0" />
               </columnConstraints>
               <rowConstraints>
